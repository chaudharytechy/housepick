<%- include('../admin-sidebar.ejs') %>


<%- include('../admin-header.ejs') %>




<div class="content mt-3">
  <div class="animated fadeIn">
      <div class="row">
          <div class="col-md-12">
            <% if(message.length>0){ %>
                <div class="alert alert-success">
                  <%= message %>
                </div>
              <% } %>
              <% if(message1.length>0){ %>
                <div class="alert alert-danger">
                  <%= message1 %>
                </div>
              <% } %>
              <div class="card">
                  <div class="card-header d-flex justify-content-between">
                      <div>
                        <strong class="card-title">About</strong>
                      </div>
                  </div>
                  <div class="card-body">
                      <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                          <thead>
                              <tr>
                                  <th>Image</th>
                                  <th>Main Heading</th>
                                  <th>Description Heading</th>
                                  <th>Description</th>
                                  <th>Action</th>
                              </tr>
                          </thead>
                          <tbody>
                              <tr>
                                <td>
                                  <img src="<%= item.image.url %>" class="img-fluid" width="100px" alt="">
                                </td>
                                <td><%= item.main_heading %> </td>
                                <td><%= item.description_heading %></td>
                                <td><%= item.description %></td>
                                <td>
                                  <a href="/admin/dashboard/aboutedit/<%= item._id %>" class="btn btn-warning">Edit</a>
                                </td>
                            </tr>
                              
                          </tbody>
                      </table>
                  </div>
              </div>
          </div>
      </div>
  </div><!-- .animated -->


</div><!-- .content -->
</div><!-- /#right-panel -->
      

<%- include('../admin-footer') %>


