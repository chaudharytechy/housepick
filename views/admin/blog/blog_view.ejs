<%- include('../admin-sidebar.ejs') %>


<%- include('../admin-header.ejs') %>




<div class="content mt-3">
  <div class="animated fadeIn">
      <div class="row">
          <div class="col-md-12">
              <div class="card">
                  <div class="card-header d-flex justify-content-between">
                      <div>
                        <strong class="card-title">Blog detail</strong>
                      </div>
                  </div>
                  <div class="card-body">
                      <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                          <thead>
                          </thead>
                          <tbody>
                            <tr>
                                <th>Image</th>
                            </tr>
                            <tr>
                                <td>
                                    <img src="<%= item.image.url %>" width="500px" alt="">
                                </td>
                            </tr>
                            <tr>
                                <th>Title</th>
                            </tr>
                            <tr>
                                <td><%= item.title %></td>
                            </tr>
                            <tr>
                                <th>Description</th>
                            </tr>
                            <tr>
                                <td><%= item.description %></td>
                            </tr>
                            
                          </tbody>
                      </table>
                  </div>
              </div>
          </div>
      </div>
  </div><!-- .animated -->


</div><!-- .content -->
</div><!-- /#right-panel -->


      

<%- include('../admin-footer') %>


